{
  "name": "db-migrations",
  "version": "1.0.0",
  "description": "Knex.js migrations for MySQL",
  "main": "dist/index.js",
  "scripts": {
    "migrate:dev": "knex migrate:latest --env development",
    "migrate:dev:down": "knex migrate:down --env development",
    "rollback:dev": "knex migrate:rollback --env development",
    "migrate:prod": "knex migrate:latest --env production",
    "seed:dev": "knex seed:run --env development",
    "seed:make": "knex seed:make",
    "create-user": "tsx scripts/create-user.ts",
    "migrate-all-vendors": "tsx scripts/migrate-firstdent-settings.ts"
  },
  "author": "Julian Wilson <julianwlsn@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "knex": "^3.1.0",
    "tsx": "^4.20.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
