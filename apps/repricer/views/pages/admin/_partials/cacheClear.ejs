<div class="d-flex justify-content-between">
    <h1 class="commanHeading">Clear Cache Items :<b>
        </b></h1>
</div>
<div class="card">
    <div class="body table-responsive">
        <a onClick="purgeAllCache()" class="btn btn-success mx-1" name="btn_cache_purge_All"
            style="float: right;color: black;"><i class="fa-solid fa-trash"></i>
            &nbsp;Purge All</a> <br /><br /><br />

        <% if(itemModel.cacheItems.length>0){ %>
            <table class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th style="font-size: 20px;">Select Cache Key to clear : </th>
                        <th><select class="form-select form-control selectpicker" data-live-search="true" data-size="5"
                                id="purge_cache_key_ddl" aria-label="Default select example">
                                <% itemModel.cacheItems.forEach(function (list,idx) { %>
                                    <option <%= idx==0 ? "selected" : "" %> value="<%= list %>">
                                        <%= list %>
                                    </option>
                                    <%})%>
                            </select></th>
                            <th><a onClick="showCacheValue()" class="btn btn-info mx-1" name="btn_cache_show_value"
                                style="color: black;"><i class="fa-solid fa-binoculars"></i>
                                &nbsp;Show</a></th>
                    </tr>
                </thead>
            </table>
            <a onClick="deleteCacheItem()" class="btn btn-warning mx-1" name="btn_cache_delete_key"
                style="float: right;color: black;"><i class="fa-solid fa-trash"></i>
                &nbsp;Remove</a> <br /><br /><br />
            <% } else { %>
                <h2>No Cache Found !!</h2>
                <% } %>
    </div>
</div>