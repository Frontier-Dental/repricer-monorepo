<div class="d-flex justify-content-between">
    <h1 class="commanHeading">Run Specific Cron :<b>
        </b></h1>
</div>
<div class="card">
    <div class="body table-responsive">
        <table class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th style="font-size: 20px;">Select Cron to Run : </th>
                    <th><select class="form-select form-control" data-live-search="true" data-size="5"
                            id="cron_to_run_ddl" aria-label="Default select example">
                            <% itemModel.items.forEach(function (list,idx) { %>
                                <option <%=idx==0 ? "selected" : "" %> value= <%= list.CronName %>>
                                        <%= list.CronName %>
                                </option>
                                <%})%>
                        </select></th>
                    <th><a onClick="runSpecificCron()" class="btn btn-success mx-1" name="btn_cron_run"
                            style="color: black;"><i class="fa-solid fa-play"></i>
                            &nbsp;Run</a></th>
                </tr>
            </thead>
        </table>
    </div>
</div>