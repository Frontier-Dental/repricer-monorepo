<%- include('../header') %>
    <%- include('../left-sidebar') %>
 
<%
    let pgno = pageNumber + 1
    let upVal = 0, lowVal = 0
    const pages = new Array(totalPages).fill(null).map((v, i) => i)
    if (pageNumber <= 2) {
      upVal = 0; lowVal = 5
    } else if (pageNumber >= (totalPages - 3)) {
      upVal = totalPages - 5; lowVal = totalPages
    } else {
      upVal = pageNumber - 2; lowVal = pageNumber + 3
    }
%>
        <div class="page">
            <div class="container-fluid">
                <div class="d-flex justify-content-between">
                    <h1 class="commanHeading">Cron History & Filter Logs</h1><br>
                </div>

                <div class="card">

                    <div class="body">
                        <h5> <b> Cron History</b></h5>
                        <table>
                               
                               <tr>
                              
                                <td>Start date</td>
                                <td>End date</td>
                                <td>Cron Name</td>
                                <td>Cron Type</td>
                                <td>Total Records</td>
                                <!-- <td>Item Per Page</td> -->
                                <td></td>
                               </tr>
                               
                               <tr>
                                <td>
                                    <input id="cHstartDate" placeholder="start date" type="datetime-local" value="<%= params.filterStartDate %>" class="form-select form-control" >
                                </td>
                                <td>
                                    <input id="cHendDate" placeholder="end date" type="datetime-local" value="<%= params.filterEndDate %>" class="form-select form-control">
                                </td>
                                <td>
                                    <select id="cHcronId" class="form-select form-control">
                                        <option value="ALL" <%= params.filterCron=='ALL'?'selected':'' %>>All</option>
                                        <% if(cronSettings.length!=0) { %>
                                            <%  cronSettings.forEach(function(item,i){ %>
                                                <option value="<%= item.CronId %>" <%= params.filterCron== item.CronId ?'selected':'' %>><%= item.CronName    %></option>
                                        <% });  } %>
                                    </select>
                                </td>
                                <td>
                                    <select id="cHcronType" class="form-select form-control">
                                        <% if(cronTypes.length!=0) { %>
                                            <%  cronTypes.forEach(function(item,i){ %>
                                                <option value="<%= item %>" <%= params.filterCronType== item ?'selected':'' %>   ><%= item %></option>
                                        <% });  } %>
                                    </select>
                                </td>
                                <td>
                                       <input  id="cHTotalRecords" class="form-select form-control" value="<%= params.filterTotalRecords == 0 ? 25: params.filterTotalRecords  %>" />
                                </td>
                                <!-- <td>
                                       <input  id="cHpageSize" class="form-select form-control" value="<%= params.filterPgSize == 0 ? 10: params.filterPgSize  %>" />
                                </td> -->
                                <td>
                                       <input type="button" id="btnSearchCronHistory"  value="Search" placeholder="end date" class="form-control btn btn-success">
                                </td>
                               </tr>
                        </table>
                    
                        </div>
                    <div class="body table-responsive" id="cronHistoryTable">
                        
                       
                        <table id="example" class="table table-striped table-bordered" style="width: 100%">
                           
                            <thead>
                                <tr>
                                    <th class="t-head">Date Time</th>
                                    <th class="t-head">Cron Completion Time</th>
                                    <th class="t-head">Cron </th>
                                    <th class="t-head">Object Id</th>
                                    <th class="t-head">Active Items</th>
                                    <th class="t-head">Eligible Count</th>
                                    <th class="t-head">Scraped-Success</th>
                                    <th class="t-head">Scraped-Failed</th>
                                    <th class="t-head">Repriced-Success</th>
                                    <th class="t-head">Repriced-422</th>
                                    <th class="t-head">Repriced-Other Errors</th>
                                    <th class="t-head">Cron Type</th>
                                    <th class="t-head">Action</th>
                                </tr>
                            </thead>
                            <tbody id="tbodyCronLogsHistory">
                                <% if(items){ items.forEach(function(item,idx){ %>
                                    <tr>
                                   
                                        <td>
                                            <%= item.logTime %> 
                                        </td>
                                        <td>
                                            <%= item.completionTime %>
                                        </td>
                                        <td>
                                            <%= item.cronName %>
                                        </td>
                                        <td>
                                            <%= item.logData._id %> ( <%= item.keyRef %> )
                                        </td>
                                        <td>
                                            <%= item.totalActiveCount %>
                                        </td>
                                        <td>
                                            <%= item.productCount %>
                                        </td>
                                        <td>
                                            <%= item.successScrapeCount %>
                                        </td>
                                        <td>
                                            <%= item.failureScrapeCount %>
                                        </td>
                                        <td>
                                            <%= item.repricedProductCount %>
                                        </td>
                                        <td>
                                           <%= item.repriceFailure422Count %>
                                        </td>
                                        <td>
                                            <%= item.repriceFailureOtherCount %>
                                        </td>
                                        <td>
                                            <% if (item.logData.type=="FEED_RUN") { %>
                                                <span class="badge badge-primary">
                                                    <%= item.logData.type %>
                                                </span>
                                            <% }else if (item.logData.type) { %>
                                            <span class="badge badge-warning"><%= item.logData.type %></span>
                                            <% } else { %>
                                            <span class="badge badge-success">Regular</span>
                                            <% } %>
                                        </td>
                                        <td>
                                            <% if(item.type && item.type=="SCRAPE_ONLY"){ %>
                                                    -
                                                <%} else { %>
                                            <a href="/dashboard/get_logs_details/<%= item.logData._id %>" target="_blank"><i
                                                    class="ti-arrow-circle-right"></i></a>
                                                <% } %>
                                        </td>
                                    </tr>
                                    <% }); } %>
                            </tbody>
                        </table>
                    </div>
                    <% if(typeof showSearchMessage !== 'undefined' && showSearchMessage && items.length === 0) { %>
                        <div class="text-center p-4" style="background-color: #f5f5f5; margin: 0; min-height: 300px; display: flex; align-items: center; justify-content: center;">
                            <p style="font-size: 18px; margin: 0; color: rgb(0, 0, 0);">Please select your search criteria and press "Search"</p>
                        </div>
                    <% } %>



                    <!-- pagination -->
                  <div class="card border-0 flex-column justify-content-between p-2 track-ccard-b bg-transparent">
                      <div md="" class="col mt-4 text-center">
                        <nav aria-label="" class="">
                          <ul class="pagination  justify-content-center">
                            <li class="page-item p-2">
                              <a href="/dashboard/cronHistory?pgno=1&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>"><button class="btn tracknumlisting-pagination-button" >&laquo;</button></a>
                            </li>
                            <li class="page-item p-2">
                              <a href="/dashboard/cronHistory?pgno=<%= Math.max(1, pageNumber) %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>"><button class="btn tracknumlisting-pagination-button" >Previous</button></a>
                            </li>
                            <% pages.slice(upVal, lowVal).map(pg => {
                              let active = (pg === pageNumber) ? '-active' : ''
                              %>
                                <li class="page-item p-2">
                                  <a href="/dashboard/cronHistory?pgno=<%= (pg + 1) %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>" ><button class="btn tracknumlisting-pagination-button" >
                                    <%= (pg + 1) %>
                                  </button></a>
                                </li>

                            <% }) %>
                            <li class="page-item p-2">
                              <a href="/dashboard/cronHistory?pgno=<%= Math.min(totalPages, Number(pageNumber) + 2) %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>"><button class="btn tracknumlisting-pagination-button" >Next</button></a>
                            </li>
                            <li class="page-item p-2">
                              <a href="/dashboard/cronHistory?pgno=<%= totalPages %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>"><button class="btn tracknumlisting-pagination-button" >&raquo;</button></a>
                            </li>
                          </ul>
                        </nav>
                      </div>
                    </div>
                </div>

                <div class="container-fluid" id="Cron-422CronDiv">
                <div class="d-flex justify-content-between">
                    <h1 class="commanHeading">Filter Cron Logs : &nbsp; <input type="number" style="width: 100px;" id="txtFilterLogCount" value="10" onfocusout="getFilterCronLogs()"/> Logs</h1>
                </div>
                <div class="card">
                    <div class="body table-responsive" id="filterCronLogs">
                        <table id="example" class="table table-striped table-bordered" style="width: 100%">
                            <thead>
                                <th class="t-head">
                                   Start Time 
                                </th>
                                <th class="t-head">
                                    End Time
                                </th>
                                <th class="t-head">
                                    Cron Name
                                </th>
                                <th class="t-head">
                                    Cron Run Id
                                </th>
                                <th class="t-head">
                                    Filter Date
                                </th>
                                <th class="t-head">
                                    No. Of Products
                                </th>
                                <th class="t-head">
                                    Action
                                </th>
                            </thead>
                            <tbody>
                                <% if(filterLogs){ filterLogs.forEach(function(item,idx){ %>
                                    <tr>
                                        <td><%= item.startTime %></td>
                                        <td><%= item.endTime %></td>
                                        <td><%= item.cronName %></td>
                                        <td><%= item._id %></td>
                                        <td><%= item.filterDate %></td>
                                        <td><%= item.countOfProducts %></td>
                                        <td><a href="/filter/export_log/<%=item.cronKey%>">Export</a></td>
                                    </tr>
                                    <% }); } %>
                            </tbody>
                        </table>
                    </div>
                </div>
                </div>
            </div>
        </div>


 

        <%- include("../footer"); %>
        <style>
                /* Basic Rules */
                .switch input {
                    display: none;
                }

                .switch {
                    display: inline-block;
                    width: 52px;
                    height: 22px;
                    margin: 8px;
                    transform: translateY(50%);
                    position: relative;
                }

                /* Style Wired */
                .slider {
                    position: absolute;
                    top: 0;
                    bottom: 0;
                    left: 0;
                    right: 0;
                    border-radius: 30px;
                    box-shadow: 0 0 0 2px #777, 0 0 4px #777;
                    cursor: pointer;
                    border: 4px solid transparent;
                    overflow: hidden;
                    transition: 0.4s;
                }

                .slider:before {
                    position: absolute;
                    content: "";
                    width: 100%;
                    height: 100%;
                    background: #777;
                    border-radius: 30px;
                    transform: translateX(-30px);
                    transition: 0.4s;
                }

                input:checked+.slider:before {
                    transform: translateX(30px);
                    background: limeGreen;
                }

                input:checked+.slider {
                    box-shadow: 0 0 0 2px limeGreen, 0 0 2px limeGreen;
                }

                /* Style Flat */
                .switch.flat .slider {
                    box-shadow: none;
                }

                .switch.flat .slider:before {
                    background: #fff;
                }

                .switch.flat input:checked+.slider:before {
                    background: white;
                }

                .switch.flat input:checked+.slider {
                    background: limeGreen;
                }
                .t-head {
                    position: sticky;
                    top: 0px;
                    left: 0px;
                    z-index: 9;
                    background-color: #fff;
                    }
                .table-responsive{
                    max-height: 700px;
                    margin: 15px;
                    padding: 0 !important;
                }
            </style>
<script src="/javascripts/dashboard.js"></script>
