                              %>
                                <li class="page-item p-2">
                                  <a href="/cronHistory?pgno=<%= (pg + 1) %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>" ><button class="btn tracknumlisting-pagination-button" >
                                    <%= (pg + 1) %>
                                  </button></a>
                                </li>
                              
                            <% }) %>
                            <li class="page-item p-2">
                              <a href="/cronHistory?pgno=<%= Math.min(totalPages, Number(pageNumber) + 2) %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>"><button class="btn tracknumlisting-pagination-button" >Next</button></a>
                            </li>
                            <li class="page-item p-2">
                              <a href="/cronHistory?pgno=<%= totalPages %>&cronType=<%= params.filterCronType %>&cronId=<%= params.filterCron %>&fromDate=<%= params.filterStartDate %>&toDate=<%= params.filterEndDate %>&totalRecords=<%= params.filterTotalRecords %>"><button class="btn tracknumlisting-pagination-button" >&raquo;</button></a>
                            </li>
                          </ul>
                        </nav>
                      </div>
                    </div>
                </div>

