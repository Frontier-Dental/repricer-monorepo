                    <div class="body">
                        <h5> <b> Cron History</b></h5>
                        <table>
                               
                               <tr>
                              
                                <td>Start date</td>
                                <td>End date</td>
                                <td>Cron Name</td>
                                <td>Cron Type</td>
                                <td>Total Records</td>
                                <!-- <td>Item Per Page</td> -->
                                <td></td>
                               </tr>
                               
                               <tr>
                                <td>
                                    <input id="cHstartDate" placeholder="start date" type="datetime-local" value="<%= params.filterStartDate %>" class="form-select form-control" >
                                </td>
                                <td>
                                    <input id="cHendDate" placeholder="end date" type="datetime-local" value="<%= params.filterEndDate %>" class="form-select form-control">
                                </td>
                                <td>
                                    <select id="cHcronId" class="form-select form-control">
                                        <option value="ALL" <%= params.filterCron=='ALL'?'selected':'' %>>All</option>
                                        <% if(cronSettings.length!=0) { %>
                                            <%  cronSettings.forEach(function(item,i){ %>
                                                <option value="<%= item.CronId %>" <%= params.filterCron== item.CronId ?'selected':'' %>><%= item.CronName    %></option>
                                        <% });  } %>
                                    </select>
                                </td>
                                <td>
                                    <select id="cHcronType" class="form-select form-control">
                                        <% if(cronTypes.length!=0) { %>
                                            <%  cronTypes.forEach(function(item,i){ %>
                                                <option value="<%= item %>" <%= params.filterCronType== item ?'selected':'' %>   ><%= item %></option>
                                        <% });  } %>
                                    </select>
                                </td>
                                <td>
                                       <input  id="cHTotalRecords" class="form-select form-control" value="<%= params.filterTotalRecords == 0 ? 25: params.filterTotalRecords  %>" />
                                </td>
                                <!-- <td>
                                       <input  id="cHpageSize" class="form-select form-control" value="<%= params.filterPgSize == 0 ? 10: params.filterPgSize  %>" />
                                </td> -->
                                <td>
                                       <input type="button" id="btnSearchCronHistory"  value="Search" placeholder="end date" class="form-control btn btn-success">
                                </td>
                               </tr>
                        </table>
                    
                        </div>
                    <div class="body table-responsive" id="cronHistoryTable">
                        
                       
                        <table id="example" class="table table-striped table-bordered" style="width: 100%">
                           
                            <thead>
                                <tr>
                                    <th class="t-head">Date Time</th>
                                    <th class="t-head">Cron Completion Time</th>
                                    <th class="t-head">Cron </th>
                                    <th class="t-head">Object Id</th>
                                    <th class="t-head">Active Items</th>
                                    <th class="t-head">Eligible Count</th>
                                    <th class="t-head">Scraped-Success</th>
                                    <th class="t-head">Scraped-Failed</th>
                                    <th class="t-head">Repriced-Success</th>
                                    <th class="t-head">Repriced-422</th>
                                    <th class="t-head">Repriced-Other Errors</th>
                                    <th class="t-head">Cron Type</th>
                                    <th class="t-head">Action</th>
                                </tr>
                            </thead>
                            <tbody id="tbodyCronLogsHistory">
                                <% if(items){ items.forEach(function(item,idx){ %>
                                    <tr>
                                   
                                        <td>
                                            <%= item.logTime %> 
                                        </td>
                                        <td>
                                            <%= item.completionTime %>
                                        </td>
                                        <td>
                                            <%= item.cronName %>
                                        </td>
                                        <td>
                                            <%= item.logData._id %> ( <%= item.keyRef %> )
                                        </td>
                                        <td>
                                            <%= item.totalActiveCount %>
                                        </td>
                                        <td>
                                            <%= item.productCount %>
                                        </td>
                                        <td>
                                            <%= item.successScrapeCount %>
                                        </td>
                                        <td>
                                            <%= item.failureScrapeCount %>
                                        </td>
                                        <td>
                                            <%= item.repricedProductCount %>
                                        </td>
                                        <td>
                                           <%= item.repriceFailure422Count %>
                                        </td>
                                        <td>
                                            <%= item.repriceFailureOtherCount %>
                                        </td>
                                        <td>
                                            <% if (item.logData.type=="FEED_RUN") { %>
                                                <span class="badge badge-primary">
                                                    <%= item.logData.type %>
                                                </span>
                                            <% }else if (item.logData.type) { %>
                                            <span class="badge badge-warning"><%= item.logData.type %></span>
                                            <% } else { %>
                                            <span class="badge badge-success">Regular</span>
                                            <% } %>
                                        </td>
                                        <td>
                                            <% if(item.type && item.type=="SCRAPE_ONLY"){ %>
                                                    -
                                                <%} else { %>
                                            <a href="/dashboard/get_logs_details/<%= item.logData._id %>" target="_blank"><i
                                                    class="ti-arrow-circle-right"></i></a>
                                                <% } %>
                                        </td>
                                    </tr>
                                    <% }); } %>
                            </tbody>
                        </table>
                    </div>


                    
                    <!-- pagination -->
                  <div class="card border-0 flex-column justify-content-between p-2 track-ccard-b bg-transparent">
