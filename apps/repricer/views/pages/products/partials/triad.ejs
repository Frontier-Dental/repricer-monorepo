
<div class="card">
    <div class="card-body">
      <% if(model.triadDetails!=null){ %>
        <form method="post" autocomplete="off" id="update_product_form_triad">
          <input type="hidden" value="Triad" name="vendors" />
          <input type="hidden" value="<%= model.mpId %>" name="mpid" />
          <div class="row no-gutters">
            <div class="form-group col-3">
              <label for="channel_name" class="font-weight-bold">
                Channel Name <span style="color: red;font-weight: bold;">*</span>
              </label>
              <input type="text" class="form-control" value="<%= model.triadDetails.channelName %>" name="channel_name"
                placeholder="Channel Name" />
            </div>
            <div class="form-group col-3">
              <label for="executionPriority" class="font-weight-bold">Execution Priority:</label>
              <br>
              <select class="form-select form-control" name="executionPriority" aria-label="Default select example">
                <option <%=model.triadDetails.executionPriority==1 ? "selected" : "" %> value=1>1</option>
                <option <%=model.triadDetails.executionPriority==2 ? "selected" : "" %> value=2>2</option>
                <option <%=model.triadDetails.executionPriority==3 ? "selected" : "" %> value=3>3</option>
                <option <%=model.triadDetails.executionPriority==4 ? "selected" : "" %> value=4>4</option>
                <option <%=model.triadDetails.executionPriority==5 ? "selected" : "" %> value=5>5</option>
                <option <%=model.triadDetails.executionPriority==6 ? "selected" : "" %> value=6>6</option>
                <option <%=model.triadDetails.executionPriority==7 ? "selected" : "" %> value=7>7</option>
              </select>
            </div>
            <div class="form-group col-3">
              <label for="percentageIncrease" class="font-weight-bold">Reprice Up %</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.percentageIncrease %>"
                name="percentageIncrease" placeholder="Reprice Up %" min="0" max="100" />
            </div>
            <div class="form-group col-3" name="divBadgePercentage">
              <label for="badgePercentage" class="font-weight-bold">Reprice UP Badge Percentage %</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.badgePercentage %>"
                name="badgePercentage" placeholder="Badge Percentage (%)" min="0" max="100" />
            </div>
          </div>
          <div class="row no-gutters">
            <div class="form-group col-3">
              <label for="sisterVendorId" class="font-weight-bold">Sister Vendor Id (Separated by
                [;])</label>
              <input type="text" class="form-control" value="<%= model.triadDetails.sisterVendorId %>"
                name="sisterVendorId" placeholder="Sister Vendor Id" />
            </div>
            <div class="form-group col-3">
              <label for="inactiveVendorId" class="font-weight-bold">Inactive Vendor Id (Separated by
                [;])</label>
              <input type="text" class="form-control" value="<%= model.triadDetails.inactiveVendorId %>"
                name="inactiveVendorId" placeholder="Inactive Vendor Id" />
            </div>
            <div class="form-group col-3">
              <label for="override_bulk_rule_select" class="font-weight-bold">Override Bulk Update (Up/Down)
                :</label>
              <select class="form-select form-control" name="override_bulk_rule_select"
                aria-label="Default select example">
                <option <%=model.triadDetails.override_bulk_rule==-1 ? "selected" : "" %> value=-1>-- Please Select --</option>
                <option <%=model.triadDetails.override_bulk_rule==0 ? "selected" : "" %> value=0>Only Up</option>
                <option <%=model.triadDetails.override_bulk_rule==1 ? "selected" : "" %> value=1>Only Down</option>
                <option <%=model.triadDetails.override_bulk_rule==2 ? "selected" : "" %> value=2>Both</option>
              </select>
            </div>
            <div class="form-group col-3">
              <label for="handling_time_filter" class="font-weight-bold">Handling Time Group :</label>
              <select class="form-select form-control" name="handling_time_filter" aria-label="Default select example"
                onchange="enablePercentageData(this)">
                <option <%=model.triadDetails.handlingTimeFilter=='ALL' ? "selected" : "" %> value='ALL'>All</option>
                <option <%=model.triadDetails.handlingTimeFilter=='FAST_SHIPPING' ? "selected" : "" %>
                  value='FAST_SHIPPING'>1-2 days (Fast Shipping)</option>
                <option <%=model.triadDetails.handlingTimeFilter=='STOCKED' ? "selected" : "" %> value='STOCKED'>1-5
                  days (Stocked)</option>
                <option <%=model.triadDetails.handlingTimeFilter=='LONG_HANDLING' ? "selected" : "" %>
                  value='LONG_HANDLING'>6+ days (Long Handling)</option>
              </select>
            </div>
          </div>
          <div class="row no-gutters">
            <div class="form-group col-3">
              <label for="excludedVendors" class="font-weight-bold">Exclude Vendor(s) (Separated by
                [;])</label>
              <input type="text" class="form-control" value="<%= model.triadDetails.excludedVendors %>"
                name="excludedVendors" placeholder="Excluded Vendor Ids" />
            </div>
            <div class="form-group col-3">
              <label for="inventoryThreshold" class="font-weight-bold">Inventory Competition Threshold</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.inventoryThreshold %>"
                name="inventoryThreshold" placeholder="Inventory Competition Threshold" min="0" max="100" />
            </div>
            <div class="form-group col-3">
              <label for="percentageDown" class="font-weight-bold">Reprice Down %</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.percentageDown %>"
                name="percentageDown" placeholder="Reprice Down %" min="0" max="1" />
            </div>
            <div class="form-group col-3">
              <label for="badgePercentageDown" class="font-weight-bold">Reprice Down Badge %</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.badgePercentageDown %>"
                name="badgePercentageDown" placeholder="Reprice Down Badge %" min="0" max="1" step="0.001" />
            </div>
            <div class="form-group col-3">
              <label for="ownVendorThreshold" class="font-weight-bold">Own Vendor Threshold</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.ownVendorThreshold %>"
                name="ownVendorThreshold" placeholder="Own Vendor Threshold" min="0" step="1" />
            </div>
            <div class="form-group col-3">
              <label for="badgePercentageDown" class="font-weight-bold">Get BB - Shipping %</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.getBBShippingValue %>"
                name="getBBShippingValue" placeholder="Get BB - Shipping %" min="0" max="1" step="0.001" />
            </div>
            <div class="form-group col-3">
              <label for="badgePercentageDown" class="font-weight-bold">Get BB - Badge %</label>
              <input type="number" class="form-control" value="<%= model.triadDetails.getBBBadgeValue %>" name="getBBBadgeValue"
                placeholder="Get BB - Badge %" min="0" max="1" step="0.001" />
            </div>
          </div>
          <div class="row no-gutters">
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="Scrape_on_off" <%=model.triadDetails.scrapeOn==true ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">
                Reprice - Scrape <span style="color: red;font-weight: bold;">*</span>
              </label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="Reprice_on_off" <%=model.triadDetails.allowReprice==true ? 'checked' : ''
                  %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Reprice</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="suppressPriceBreak" <%=model.triadDetails.suppressPriceBreak==true
                  ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Suppress Price Break</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="competeAll" <%=model.triadDetails.competeAll==true ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Compete with all vendors</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="compareWithQ1" <%=model.triadDetails.compareWithQ1==true ? 'checked' : ''
                  %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Q2 compete with Q1</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="applyBuyBox" <%=model.triadDetails.applyBuyBoxLogic==true ? 'checked' : ''
                  %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Apply Buy Box</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="applyNcForBuyBox" <%=model.triadDetails.applyNcForBuyBox==true ? 'checked'
                  : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Apply NC For Buy Box</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="includeInactiveVendors" <%=model.triadDetails.includeInactiveVendors==true
                  ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Include Inactive Vendors (On/Off)</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="override_bulk_update" <%=model.triadDetails.override_bulk_update==true
                  ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Override Bulk Update (On/Off)</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="suppressPriceBreakForOne"
                  <%=model.triadDetails.suppressPriceBreakForOne==true ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Suppress Price Break if Qty 1 not
                updated</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="beatQPrice" <%=model.triadDetails.beatQPrice==true ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Compete On Price Breaks Only</label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="keepPosition" <%=model.triadDetails.keepPosition==true ? 'checked' : '' %>
                />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">Keep Position</label>
            </div>
          </div>
          <div class="row no-gutters">
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="competeWithNext" <%=model.triadDetails.competeWithNext==true ? 'checked'
                  : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">
                Floor-Compete With Next <span style="color: red;font-weight: bold;">*</span>
              </label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="ignorePhantomQBreak" <%=model.triadDetails.ignorePhantomQBreak==true ? 'checked' : ''
                  %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">
                Ignore Phantom Q Break <span style="color: red;font-weight: bold;">*</span>
              </label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="getBBShipping" <%=model.triadDetails.getBBShipping==true ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">
                Get BB - Shipping <span style="color: red;font-weight: bold;">*</span>
              </label>
            </div>
            <div class="form-group col-3">
              <label class="switch">
                <input type="checkbox" name="getBBBadge" <%=model.triadDetails.getBBBadge==true ? 'checked' : '' %> />
                <span class="slider"></span>
              </label>
              <label for="" class="font-weight-bold">
                Get BB - Badge <span style="color: red;font-weight: bold;">*</span>
              </label>
            </div>
          </div>
          <button type="submit" class="btn btn-success mx-1" style="float: right;color: antiquewhite;">
            Save Changes
          </button>
        </form>
        <% } else { %>
          <h5 id="show_text_triad">No Data</h5>
          <% } %>
    </div>
  </div>